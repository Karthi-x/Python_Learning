def turn_right():
    turn_left()
    turn_left()
    turn_left()
    

while not front_is_clear():
    turn_left()
put()  
move()
    
while not object_here("token"):
    if wall_on_right() and front_is_clear():
        if wall_on_right() and front_is_clear():
